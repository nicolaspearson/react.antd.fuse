{
	"name": "react.antd.fuse",
	"version": "0.0.1",
	"description": "Starter project for React built using Typescript, MobX, Ant Design, and FuseBox.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nicolaspearson/react.antd.fuse.git"
	},
	"license": "GPL-3.0+",
	"scripts": {
		"fuse": "ts-node --project fuse/tsconfig.json fuse/fuse.ts",
		"clean": "npm run fuse clean",
		"start": "NODE_ENV=development npm run fuse",
		"build": "NODE_ENV=production npm run fuse build",
		"prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
	},
	"dependencies": {
		"antd": "~3.10.9",
		"classnames": "~2.2.6",
		"mobx": "~5.6.0",
		"mobx-react": "~5.4.2",
		"react": "~16.6.3",
		"react-dom": "~16.6.3",
		"router5": "~6.6.2"
	},
	"devDependencies": {
		"@types/classnames": "~2.2.6",
		"@types/node": "~10.12.10",
		"@types/react": "~16.7.8",
		"@types/react-dom": "~16.0.11",
		"antd-iconfont": "~3.0.3",
		"dotenv": "~6.1.0",
		"dotenv-expand": "~4.2.0",
		"fuse-box": "~3.6.0",
		"fuse-box-typechecker": "~2.11.0",
		"less": "~3.9.0",
		"less-vars-to-js": "~1.3.0",
		"prettier": "~1.15.2",
		"ts-node": "~7.0.1",
		"tslint": "~5.11.0",
		"tslint-config-prettier": "~1.17.0",
		"tslint-react": "~3.6.0",
		"typescript": "~3.2.1",
		"uglify-es": "~3.3.9",
		"uglify-js": "~3.4.9"
	}
}
